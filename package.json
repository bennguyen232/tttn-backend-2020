{
  "name": "tttn-backend-2020",
  "version": "1.0.0",
  "description": "ThucTap",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t tttn-backend-2020 .",
    "docker:run": "docker run -p 3000:3000 -d tttn-backend-2020",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "dev": "npm run clean && npm start",
    "migrate:db": "npm run clean && npm run build && npm run migrate"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^6.0.0",
    "@loopback/authentication-jwt": "^0.5.1",
    "@loopback/boot": "^2.5.0",
    "@loopback/core": "^2.9.4",
    "@loopback/repository": "^2.11.1",
    "@loopback/rest": "^6.1.0",
    "@loopback/rest-explorer": "^2.2.9",
    "@loopback/service-proxy": "^2.3.7",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.20",
    "loopback-connector-mssql": "^3.8.0",
    "md5": "^2.3.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.1",
    "@loopback/eslint-config": "^9.0.1",
    "@loopback/testlab": "^3.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^10.17.28",
    "@types/passport": "^1.0.4",
    "@types/passport-http": "^0.3.8",
    "eslint": "^7.6.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.7"
  }
}
